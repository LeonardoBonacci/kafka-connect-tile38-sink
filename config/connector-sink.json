{
  "name": "tile",
  "config": {
    "connector.class": "guru.bonacci.kafka.connect.Tile38SinkConnector",
    "topics": "foo,bar",

    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter.schemas.enable": "false",

    "command.foo": "foo event.id FIELD route event.route POINT event.lat event.lon",
    "command.bar": "bar event.id FIELD route event.route POINT event.lat event.lon",

    "tile38.url": "tile38",
    "tile38.port": 9851,

    "tasks.max": "1"
  }
}
